{
  "name": "react-web",
  "version": "1.0.0",
  "description": "一个 React 的网页项目",
  "keywords": [
    "html",
    "css",
    "js",
    "react",
    "web"
  ],
  "author": "wudajian",
  "license": "MIT",
  "main": "src/index.js",
  "homepage": "https://github.com/DaJianWu/react-web#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaJianWu/react-web.git"
  },
  "bugs": {
    "url": "https://github.com/DaJianWu/react-web/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{css,less,scss}": "stylelint --fix",
    "*.": "prettier --write"
  },
  "scripts": {
    "type-check": "tsc --noemit",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "stylelint": "stylelint src/**/*.{css,less,scss}",
    "prettier": "prettier src/**/* --check",
    "lint": "npm run eslint && npm run stylelint && npm run prettier",
    "prepare": "husky install",
    "postcss": "postcss src/**/*.{css,less,scss} --base src --dir build",
    "babel": "babel src --extensions .js,.jsx,.ts,.tsx --out-dir lib",
    "start": "webpack serve --mode development --env APP_ENV=dev",
    "build": "webpack build --mode production --env APP_ENV=prod"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.29.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.7",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.3",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.5.4",
    "webpack": "^5.69.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/runtime": "^7.17.2",
    "antd": "^4.19.3",
    "core-js": "^3.21.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "regenerator-runtime": "^0.13.9"
  }
}
