{
  "name": "react-web",
  "version": "1.0.0",
  "description": "一个 React 的网页项目",
  "keywords": [
    "html",
    "css",
    "js",
    "react",
    "web"
  ],
  "author": "wudajian",
  "license": "MIT",
  "main": "src/index.js",
  "homepage": "https://github.com/DaJianWu/react-web#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaJianWu/react-web.git"
  },
  "bugs": {
    "url": "https://github.com/DaJianWu/react-web/issues"
  },
  "scripts": {
    "start": "webpack serve --env APP_ENV=dev",
    "build": "webpack build --env APP_ENV=prod",
    "debug": "webpack build --env APP_ENV=debug",
    "tsc": "tsc --noemit",
    "postcss": "postcss src/**/*.{css,less,scss} --base src --dir build",
    "babel": "babel src --extensions .js,.jsx,.ts,.tsx --out-dir lib",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "stylelint": "stylelint src/**/*.{css,less,scss}",
    "prettier": "prettier src/**/* --check",
    "lint": "npm run eslint && npm run stylelint && npm run prettier",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "7.x",
    "@babel/core": "7.x",
    "@babel/plugin-transform-runtime": "7.x",
    "@babel/preset-env": "7.x",
    "@babel/preset-react": "7.x",
    "@babel/preset-typescript": "7.x",
    "@commitlint/cli": "16.x",
    "@commitlint/config-conventional": "16.x",
    "@types/lodash": "4.x",
    "@types/node": "20.x",
    "@types/react": "17.x",
    "@types/react-dom": "17.x",
    "@types/react-router-dom": "5.x",
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "babel-loader": "8.x",
    "copy-webpack-plugin": "10.x",
    "css-loader": "6.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-react": "7.x",
    "html-webpack-plugin": "5.x",
    "husky": "7.x",
    "less": "4.x",
    "less-loader": "10.x",
    "lint-staged": "12.x",
    "lodash": "4.x",
    "mini-css-extract-plugin": "2.x",
    "postcss": "8.x",
    "postcss-cli": "9.x",
    "postcss-less": "6.x",
    "postcss-loader": "6.x",
    "postcss-preset-env": "7.x",
    "postcss-scss": "4.x",
    "prettier": "2.x",
    "sass": "1.x",
    "sass-loader": "12.x",
    "style-loader": "3.x",
    "stylelint": "14.x",
    "stylelint-config-prettier": "9.x",
    "stylelint-config-standard": "24.x",
    "thread-loader": "4.x",
    "typescript": "4.x",
    "webpack": "5.x",
    "webpack-bundle-analyzer": "4.x",
    "webpack-cli": "4.x",
    "webpack-dev-server": "4.x",
    "webpack-merge": "5.x"
  },
  "dependencies": {
    "@ant-design/icons": "4.x",
    "@babel/runtime": "7.x",
    "antd": "4.x",
    "axios": "0.x",
    "core-js": "3.x",
    "react": "18.x",
    "react-dom": "18.x",
    "react-redux": "7.x",
    "react-router-dom": "5.x",
    "redux": "4.x",
    "redux-thunk": "2.x",
    "regenerator-runtime": "0.x"
  }
}
